#!/bin/bash

set -e
set -o pipefail

MINPARAMS=1
if [ $# -lt "$MINPARAMS" ]
then
    echo usage:
    echo   buildclean "option" 
    echo "Options are: all linux cloudinfra media"
    echo "routing packages extra fwc cce vl access"
    echo
    echo i.e.:
    echo   buildclean media
    echo    
    exit 1
fi

TOP=`pwd`
echo Source Base=$TOP

BUILDTYPE=$1

function clean_nos {

    echo "------------- cleaning nos -------------"

    rm -rf $TOP/images/nos
    rm -rf $TOP/buildoutput/nos

    mkdir $TOP/images/nos
    mkdir $TOP/buildoutput/nos
}

function clean_linux {

    echo "------------- cleaning linux -------------"

    rm -rf $TOP/images/linux
    rm -rf $TOP/buildoutput/linux

    mkdir $TOP/images/linux
    mkdir $TOP/buildoutput/linux
}

function clean_lxde {

    echo "------------- cleaning lxde -------------"

    rm -rf $TOP/images/lxde
    rm -rf $TOP/buildoutput/lxde

    mkdir $TOP/images/lxde
    mkdir $TOP/buildoutput/lxde

    echo "------------ cleaning lxappearance  ------------"

    cd $TOP/lxde/lxappearance
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxconf  ------------"

    cd $TOP/lxde/lxconf
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxde-common  ------------"

    cd $TOP/lxde/lxde-common
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxde-icon-theme  ------------"

    cd $TOP/lxde/lxde-icon-theme
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxdm  ------------"

    cd $TOP/lxde/lxdm
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxinput  ------------"

    cd $TOP/lxde/lxinput
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxlauncher  ------------"

    cd $TOP/lxde/lxlauncher
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxmenu-data  ------------"

    cd $TOP/lxde/lxmenu-data
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxpanel  ------------"

    cd $TOP/lxde/lxpanel
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxpanel-plugins  ------------"

    cd $TOP/lxde/lxpanel-plugins/batt
    if [ -f Makefile ]
    then   
        make distclean
    fi

    cd $TOP/lxde/lxpanel-plugins/menu_2
    if [ -f Makefile ]
    then   
        make distclean
    fi

    cd $TOP/lxde/lxpanel-plugins/drives
    if [ -f Makefile ]
    then   
        make distclean
    fi

    cd $TOP/lxde/lxpanel-plugins/netstat
    if [ -f Makefile ]
    then   
        make distclean
    fi

    cd $TOP/lxde/lxpanel-plugins/xkb
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxrandr  ------------"

    cd $TOP/lxde/lxrandr
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxsession  ------------"

    cd $TOP/lxde/lxsession
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxsession-edit  ------------"

    cd $TOP/lxde/lxsession-edit
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxshortcut  ------------"

    cd $TOP/lxde/lxshortcut
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxtask  ------------"

    cd $TOP/lxde/lxtask
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning lxterminal  ------------"

    cd $TOP/lxde/lxterminal
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning menu-cache  ------------"

    cd $TOP/lxde/menu-cache
    if [ -f Makefile ]
    then   
        make distclean
    fi

}

function clean_cloudinfra {

    echo "------------- cleaning cloudinfra -------------"

    rm -rf $TOP/images/cloudinfra
    rm -rf $TOP/buildoutput/cloudinfra

    mkdir $TOP/images/cloudinfra
    mkdir $TOP/buildoutput/cloudinfra
}

function clean_media {

    echo "-------------cleaning media-------------"

    rm -rf $TOP/images/media
    rm -rf $TOP/buildoutput/media

    mkdir $TOP/images/media
    mkdir $TOP/buildoutput/media

    echo "------------ cleaning a52dec ------------"

    cd $TOP/media/a52dec
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning alsa-lib ------------"

    cd $TOP/media/alsa-lib
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning alsa-utils ------------"

    cd $TOP/media/alsa-utils
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning ffmpeg ------------"

    cd $TOP/media/ffmpeg
    if [ -f config.h ]
    then   
        make distclean
    fi

    echo "------------ cleaning flac ------------"

    cd $TOP/media/flac
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning gstreamer ------------"

    cd $TOP/media/gstreamer
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libass ------------"

    cd $TOP/media/libass
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libdvdcss ------------"

    cd $TOP/media/libdvdcss
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libdvdnav ------------"
    cd $TOP/media/libdvdnav
    if [ -f config.mak ]
    then   
        make distclean
    fi

    echo "------------ cleaning libdvdread ------------"

    cd $TOP/media/libdvdread
    if [ -f config.mak ]
    then   
        make distclean
    fi

    echo "------------ cleaning libeXosip ------------"

    cd $TOP/media/libeXosip
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "-------------cleaning libfreenect-------------"

    cd $TOP/media/libfreenect
    if [ -f Makefile ]
    then
        make clean
    fi

    if [ -f CMakeCache.txt ]
    then
        rm CMakeCache.txt
    fi

    echo "------------ cleaning libogg ------------"

    cd $TOP/media/libogg
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libosip ------------"

    cd $TOP/media/libosip
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libsndfile ------------"

    cd $TOP/media/libsndfile
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libvorbis ------------"

    cd $TOP/media/libvorbis
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning libx264 ------------"

    cd $TOP/media/libx264
    if [ -f config.mak ]
    then   
        make distclean
    fi

    echo "------------ cleaning multicat ------------"

    cd $TOP/media/multicat
    if [ -f Makefile ]
    then   
        make clean
    fi

    echo "------------ cleaning pulseaudio ------------"

    cd $TOP/media/pulseaudio
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning spandsp ------------"

    cd $TOP/media/spandsp
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning speex ------------"

    cd $TOP/media/speex
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning tiff ------------"

    cd $TOP/media/tiff
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "------------ cleaning v4l-utils ------------"

    cd $TOP/media/v4l-utils
    if [ -f Makefile ]
    then   
        make distclean
    fi

}

