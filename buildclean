function clean_media_cloud {

    echo "------------- cleaning media cloud -------------"

    rm -rf $TOP/buildoutput/media_cloud
    mkdir $TOP/buildoutput/media_cloud
    rm -rf $TOP/images/media_cloud
    mkdir $TOP/images/media_cloud
}

function clean_cce_cloud {

    echo "------------- cleaning cce cloud -------------"

    rm -rf $TOP/buildoutput/cce_cloud
    mkdir $TOP/buildoutput/cce_cloud
    rm -rf $TOP/images/cce_cloud
    mkdir $TOP/images/cce_cloud
}

function clean_all {
    rm -rf $TOP/buildoutput/arm
    rm -rf $TOP/buildoutput/mips
    rm -rf $TOP/buildoutput/i686
    rm -rf $TOP/buildoutput/x86_64
    mkdir $TOP/buildoutput/arm
    mkdir $TOP/buildoutput/mips
    mkdir $TOP/buildoutput/i686
    mkdir $TOP/buildoutput/x86_64
    clean_linux 
    clean_nos 
    clean_lxde 
    clean_cloudinfra 
    clean_media 
    clean_tpd_apps 
    clean_tpd_display 
    clean_routing 
    clean_packages 
    clean_extras
    clean_fwc 
    clean_cce 
    clean_vl 
    clean_access 
    clean_media_cloud 
    clean_cce_cloud 
}

case "$BUILDTYPE" in
    all) 
        clean_all
        ;;
    linux)
        clean_linux 
        clean_lxde 
        clean_nos
        ;;
    cloudinfra)
        clean_cloudinfra 
        ;;
    media)
        clean_media 
        ;;
    tpd)
        clean_tpd_apps 
        clean_tpd_display 
        ;;
    routing)
        clean_routing 
        ;;
    packages)
        clean_packages 
        ;;
    extra)
        clean_extras
        ;;
    fwc)
        clean_fwc 
        ;;
    cce)
        clean_cce 
        clean_cce_cloud 
        ;;
    vl)
        clean_vl 
        clean_media_cloud 
        ;;
    access)
        clean_access 
        ;;
    *)   
        echo "Choice should be: all linux cloudinfra media tpd webserver"
        echo "routing lxde nos mysql fwc cce vl access media_cloud cce_cloud"
        exit 1
        ;;
esac


rm -rf $TOP/images/builds
mkdir $TOP/images/builds

echo "---------- Print Default Config ----------------"

$TOP/Config/envtest

