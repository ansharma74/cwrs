function clean_extras {

    echo "-------------cleaning extras-------------"
    rm -rf $TOP/images/webserver
    rm -rf $TOP/buildoutput/webserver

    mkdir $TOP/images/webserver
    mkdir $TOP/buildoutput/webserver

    echo "-------------cleaning apache httpd-------------"

    cd $TOP/apache/httpd
    if [ -f Makefile ]
    then   
        make distclean
    fi

    echo "-------------cleaning mysql-------------"
    rm -rf $TOP/images/mysql
    rm -rf $TOP/buildoutput/mysql

    mkdir $TOP/images/mysql
    mkdir $TOP/buildoutput/mysql

    cd $TOP/mysql
    if [ -f Makefile ]
    then
        make clean
    fi

    if [ -f CMakeCache.txt ]
    then
        rm CMakeCache.txt
    fi

}

